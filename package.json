    "pinia": "^2.0.18",
    "vue": "^3.2.37",
    "vue-router": "^4.1.3"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.7",
    "@rushstack/eslint-patch": "^1.1.4",
    "@testing-library/vue": "^6.6.1",
    "@types/marked": "^4.0.3",
    "@types/node": "^18.7.5",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vue/compiler-sfc": "^3.2.37",
