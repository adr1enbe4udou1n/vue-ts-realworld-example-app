{
  "name": "vue-realworld-example-app",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "openapi": "openapi-typescript https://aspnetcorerealworld.okami101.io/api/v1/docs.json --output src/api/conduit.ts"
  },
  "dependencies": {
    "@tanstack/vue-query": "^5.17.2",
    "@vueuse/core": "^10.7.1",
    "@vueuse/head": "^2.0.0",
    "marked": "^11.1.1",
    "nprogress": "^0.2.0",
    "openapi-typescript-fetch": "^1.1.3",
    "pinia": "^2.1.7",
    "vue": "^3.4.5",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.27",
    "@rushstack/eslint-patch": "^1.6.1",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.10.6",
    "@types/nprogress": "^0.2.3",
    "@unocss/reset": "^0.58.3",
    "@vitejs/plugin-vue": "^5.0.2",
    "@vue/compiler-dom": "^3.4.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/runtime-core": "^3.4.5",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "jsdom": "^23.1.0",
    "npm-run-all2": "^6.1.1",
    "openapi-typescript": "^6.7.3",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "unocss": "^0.58.3",
    "unplugin-auto-import": "^0.17.3",
    "unplugin-vue-components": "^0.26.0",
    "unplugin-vue-macros": "^2.7.8",
    "unplugin-vue-router": "^0.7.0",
    "vite": "^5.0.11",
    "vitest": "^1.1.3",
    "vue-tsc": "^1.8.27"
  }
}