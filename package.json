    "openapi-typescript-fetch": "^1.1.3",
    "pinia": "^2.0.22",
    "vue": "^3.2.40",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.8",
    "@rushstack/eslint-patch": "^1.2.0",
    "@testing-library/vue": "^6.6.1",
    "@types/marked": "^4.0.7",
    "@types/node": "^18.8.3",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/compiler-sfc": "^3.2.40",
